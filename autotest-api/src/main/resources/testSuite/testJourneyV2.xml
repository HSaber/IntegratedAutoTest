<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="TestJourneyV2Suite" parallel="tests" thread-count="30">
    <test name="JourneyBasic">
        <classes>
            <class name="com.qa.Omni.Journey.TestJourneyBasic"></class>
        </classes>
    </test>
    <test name="JourneyNode">
        <classes>
            <class name="com.qa.Omni.Journey.TestJourneyNode"></class>
        </classes>
    </test>
    <test name="JourneyStartNode">
        <classes>
            <class name="com.qa.Omni.Journey.TestJourneyStartNode"></class>
        </classes>
    </test>
    <test name="JourneyActionMQ">
        <classes>
            <class name="com.qa.Omni.Journey.TestJourneyActionMQ"></class>
        </classes>
    </test>
    <!--permission -->
    <test name="journeyPermission">
        <classes>
            <class name="com.qa.permission.journeyPermission"></class>
        </classes>
    </test>
    <!--AuditProcess-->
    <test name="JourneyauditProcess">
        <classes>
            <class name="com.qa.auditProcess.JourneyauditProcess"></class>
        </classes>
    </test>
    <listeners>
        <listener class-name="com.qa.listener.ExtentTestNGIReporterListener"/>
    </listeners>
</suite>