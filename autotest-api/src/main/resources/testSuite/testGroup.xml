<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="testGroupSuite" parallel="tests" thread-count="30">
    <test name="group">
        <classes>
            <class name="com.qa.follower.group.TestGroup"></class>
        </classes>
    </test>
    <!--permission -->
    <test name="groupsPermission">
        <classes>
            <class name="com.qa.permission.groupsPermission"></class>
        </classes>
    </test>
    <listeners>
        <listener class-name="com.qa.listener.ExtentTestNGIReporterListener"/>
    </listeners>
</suite>
