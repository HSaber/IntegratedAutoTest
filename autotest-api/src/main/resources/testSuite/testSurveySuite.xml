<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="testSurveySuite" preserve-order="true" parallel="tests" thread-count="10">
    <test name="TestSurvey">
        <classes>
            <class name="com.qa.modules.b2b.TestSurvey"></class>
        </classes>
    </test>
    <test name="TestForm">
        <classes>
            <class name="com.qa.modules.b2b.TestForm"></class>
        </classes>
    </test>
    <!--permission -->
    <test name="formPermission">
        <classes>
            <class name="com.qa.permission.formPermission"></class>
        </classes>
    </test>
    <test name="formNewPermission">
        <classes>
            <class name="com.qa.permission.formNewPermission"></class>
        </classes>
    </test>
    <listeners>
        <listener class-name="com.qa.listener.ExtentTestNGIReporterListener"/>
    </listeners>
</suite>