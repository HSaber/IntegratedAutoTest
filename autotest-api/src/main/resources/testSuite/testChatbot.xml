<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="testChatbot" parallel="tests" thread-count="10">
    <test name="testChatbot">
        <classes>
            <class name="com.qa.Omni.TestChatbot"></class>
        </classes>
    </test>
    <!--permission -->
    <test name="chatbotsPermission">
        <classes>
            <class name="com.qa.permission.chatbotsPermission"></class>
        </classes>
    </test>
    <listeners>
        <listener class-name="com.qa.listener.ExtentTestNGIReporterListener"/>
    </listeners>
</suite>